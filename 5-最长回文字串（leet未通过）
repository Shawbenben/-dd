#include<stdio.h>

char * l(char * s){
    int len,a,b,c;
    int count,max,tem;
    while(s[len]!=0)    len++;
    
    for(a=0;a<len;a++){
        for(b=len-1;b>a;b--){
            if(b-a<=max) break;
            for(c=0;c<b/2 ;c++){
                if(s[a+c]==s[b-c]){
                    count+=2;
                    if(a+c==b-c)
                        count--;
                }            
                else{
				
                    count=0;
                    c=b/2+1;
                }
            }
            if(count!=0 && count>max)   {
                max=count;
                count=0;
                tem=a;
            } 
        }
    }
    *(s+tem+max)=0;
    //printf("%d",max);
    return s+tem;
}


int main(){
	char arr[]="agfksjsksgl";
	printf("%s",l(arr));
	return 0;
}
